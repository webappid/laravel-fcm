{
  "name": "webappid/laravel-fcm",
  "description": "Laravel FCM server",
  "license": "MIT",
  "authors": [
    {
      "name": "Dyan Galih",
      "email": "dyan.galih@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "WebAppId\\Fcm\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebAppId\\Fcm\\Tests\\": "src/tests"
    }
  },
  "require": {
    "php" : ">=7.2.0",
    "illuminate/support": "~5.5",
    "doctrine/dbal": "^2.5",
    "guzzlehttp/guzzle": "^6.3",
    "nesbot/carbon": "^1.33",
    "webappid/laravel-user": "~1.5.3",
    "webappid/laravel-ddd": "^1.0@dev",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit" : "7.2.6",
    "squizlabs/php_codesniffer": "3.*",
    "mockery/mockery": "dev-master@dev",
    "orchestra/testbench": "~3.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "WebAppId\\Fcm\\ServiceProvider"
      ],
      "aliases": {
        "Content" : "WebAppId\\Fcm\\Facade"
      }
    }
  }
}
